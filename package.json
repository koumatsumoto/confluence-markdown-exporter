{
  "name": "confluence-markdown-exporter",
  "version": "0.1.0",
  "description": "A tool to export Confluence pages as Markdown files",
  "engines": {
    "node": ">=23.8.0"
  },
  "type": "module",
  "main": "main.ts",
  "scripts": {
    "build": "tsc",
    "start": "node --env-file=.env main.ts",
    "test": "vitest run --coverage"
  },
  "keywords": [
