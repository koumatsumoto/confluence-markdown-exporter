    "turndown-plugin-gfm": "^1.0.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "@types/turndown": "^5.0.5",
    "@vitest/coverage-v8": "^3.0.5",
    "msw": "^2.7.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  }
